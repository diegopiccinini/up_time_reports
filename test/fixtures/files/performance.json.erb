<%
    start= Date.yesterday.to_time
    collection=[]
    24.times do |i|
       downtime = rand(600)
       downtime = 0 if downtime>300
       uptime=3600 - downtime
       unmonitored = rand(100)
       unmonitored = 0 if  unmonitored>60
       collection << { starttime: start.to_i + (i * 3600), avgresponse: rand(1000), uptime: uptime, unmonitored: unmonitored }
    end
  %>
{
  "summary" : {
    "hours" : <% collection.to_json %>
  }
}
