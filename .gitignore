# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
*.sw*
/.env
/.env.production
/.env.development
/.env.test
/config/database.yml
/config/secrets.yml
/out.txt
/public/assets
/client_secret.json
/scripts/csv/*
